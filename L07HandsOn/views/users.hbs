<h1>All Users</h1>

<ul>
    {{#each users}}
    <li>
        <a href="/users/{{UserId}}">{{Username}}</a>
        <form method="POST" action="/users/{{UserId}}" 
            onsubmit="return confirm('Are you sure you want to delete this user?');">
            <button type="submit" >Delete</button>
        </form>
    </li>
    {{/each}}
</ul>